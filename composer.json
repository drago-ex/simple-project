{
	"name": "drago-ex/simple-project",
	"type": "library",
	"description": "Basic project for create single-page sites on Drago Simple",
	"license": ["MIT"],
	"authors": [
		{
			"name": "Zdeněk Papučík",
			"homepage": "https://github.com/accgit"
		}
	],
	"require": {
		"php": ">=8.0",
		"tracy/tracy": "^2.7",
		"drago-ex/simple": "dev-master",
		"dg/composer-cleaner": "^2.1"
	},
	"require-dev": {
		"phpstan/phpstan-nette": "^0.12"
	},
	"autoload": {
		"classmap": ["app/Bootstrap.php"]
	},
	"scripts": {
		"phpstan": "phpstan analyse --level 5 --configuration phpstan.neon app"
	}
}
